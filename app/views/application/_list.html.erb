<div class="card" style="width: 18rem;">
  <div class="card-header">
    <%= list.title %></h3>
  </div>

  <ul class="list-group list-group-flush">
    <% list.books.each do |book| %>
      <li class="list-group-item"><%= link_to book.title, book_path(book.id) %></li>
    <% end %>
  </ul>
</div>


