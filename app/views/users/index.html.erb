<% if flash[:message] %>
    <%= flash[:message] %>
<% end %>
<% @users.each do |user| %>
    <%= render partial: "userInfo", locals: {person: user} %>
    <%= link_to "More about this user", user_path(user) %>
<% end %>
<br><br><br>
<%= button_to "Create new user", new_user_session_path, method: :get%>
<div>
    <%= link_to "Books *temporary*", books_path, class: "btn btn-sm btn-primary" %>
    <%= link_to "Reviews *temporary*", reviews_path, class: "btn btn-sm btn-primary" %>
</div>